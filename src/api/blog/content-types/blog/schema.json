    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "blogs"
    },
    "author": {
      "type": "component",
      "repeatable": false,
      "component": "author.item",
      "required": true
    },
    "shortText": {
      "type": "text",
